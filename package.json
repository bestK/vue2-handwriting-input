{
    "name": "@bestk/vue2-handwriting-input",
    "version": "1.0.0",
    "description": "Vue2手写输入识别组件，支持中文手写识别",
    "main": "dist/vue2-handwriting-input.umd.min.js",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "dist",
        "lib"
    ],
    "keywords": [
        "vue",
        "vue2",
        "handwriting",
        "input",
        "recognition",
        "chinese",
        "component"
    ],
    "author": "bestk",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/bestk/vue2-handwriting-input.git"
    },
    "bugs": {
        "url": "https://github.com/bestk/vue2-handwriting-input/issues"
    },
    "homepage": "https://github.com/bestk/vue2-handwriting-input#readme",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
